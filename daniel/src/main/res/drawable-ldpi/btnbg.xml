<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <corners
                android:radius="14dp"
                />
            <solid
                android:color="#46f6f7f7"
                />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp"
                />
            <size
                android:width="270dp"
                android:height="60dp"
                />
            <stroke
                android:width="3dp"
                android:color="#ffffff"
                />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners
                android:radius="14dp"
                />
            <solid
                android:color="#46f6f7f7"
                />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp"
                />
            <size
                android:width="270dp"
                android:height="60dp"
                />
            <stroke
                android:width="3dp"
                android:color="#ffffff"
                />
        </shape>
    </item>
</selector>